@model ENTIDAD.Proyecto

@{
    ViewBag.Title = "GetProyectos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h4>Proyecto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre Proyecto
        </dt>

        <dd>
            @Model.NombreProyecto)
        </dd>

        <dt>
            Fecha Inicio
        </dt>

        <dd>
            @Model.FechaInicio.ToShortDateString())
        </dd>

        <dt>
            Fecha Fin
        </dt>

        <dd>
            @Model.FechaFin.ToShortDateString())
        </dd>

    </dl>
</div>
<p>
    <a title="Editar" href="@Url.Action("Editar", "Proyecto", new { id=Model.ProyectoId })" class="btn btn-danger">
        <i class="glyphicon glyphicon-trash"></i> Eliminar
    </a>

    <a href="@Url.Action("Index", "Proyecto")" class="btn btn-info">
        <i class="glyphicon glyphicon-chevron-left"></i> Regersar
    </a>
</p>
