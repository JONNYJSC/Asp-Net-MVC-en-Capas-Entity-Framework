@model ENTIDAD.Proyecto

@{
    ViewBag.Title = "GetProyectos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h4>Proyecto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre Proyecto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreProyecto)
        </dd>

        <dt>
            Fecha Inicio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>

        <dt>
            Fecha Fin
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaFin)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Editar", new { id = Model.ProyectoId }) |
    @Html.ActionLink("Regresar", "Index")
</p>
